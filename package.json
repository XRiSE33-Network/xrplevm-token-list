{
  "name": "@xrise33/token-list",
  "version": "1.2.1",
  "type": "module",
  "description": "Community-maintained, machine-readable list of XRPL EVM tokens.",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "validate": "tsx scripts/validate.ts",
    "test": "pnpm typecheck && pnpm validate",
    "prepublishOnly": "pnpm build",
    "build": "pnpm test && tsx scripts/build.ts && tsc -p tsconfig.types.json && tsc -p tsconfig.sdk.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XRiSE33-Network/xrplevm-token-list.git"
  },
  "keywords": [
    "xrpl",
    "evm",
    "erc20",
    "json-schema",
    "xrplevm",
    "token",
    "cryptocurrency",
    "token-list"
  ],
  "author": "XRiSE33 Network",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/XRiSE33-Network/xrplevm-token-list/issues"
  },
  "homepage": "https://github.com/XRiSE33-Network/xrplevm-token-list#readme",
  "files": [
    "dist",
    "images",
    "schema"
  ],
  "exports": {
    ".": {
      "import": "./dist/tokenlist.json",
      "require": "./dist/tokenlist.json",
      "types": "./dist/types-entry.d.ts"
    },
    "./schema": {
      "types": "./dist/schema/schema.d.ts"
    },
    "./sdk": {
      "import": "./dist/src/sdk.js",
      "types": "./dist/src/sdk.d.ts"
    }
  },
  "types": "dist/types-entry.d.ts",
  "devDependencies": {
    "@types/node": "^24.0.14",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "ethers": "^6.15.0",
    "sharp": "^0.34.5",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
